{
  "name": "skeleton-react-webpack",
  "version": "1.0.0",
  "description": "Esqueleto para proyectos que usan Webpack con React",
  "private": true,
  "scripts": {
    "test": "jest --watchAll --verbose",
    "start": "webpack serve --open --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "docs": "jsdoc -c jsdoc.json"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "docdash": "^1.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "jest-cucumber": "^3.0.1",
    "jsdoc": "^3.6.7",
    "jsdoc-http-plugin": "^0.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "jest": {
    "testMatch": [
      "**/*.steps.js",
      "**/*.test.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fredypalma/skeleton-react-webpack.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fredypalma/skeleton-react-webpack/issues"
  },
  "homepage": "https://github.com/fredypalma/skeleton-react-webpack#readme"
}
